pragma solidity ^0.4.18;
//v1.1.1
import "../IexecOracle.sol";
import '../IexecLib.sol';

//for test purpuse
contract IexecOracleAPIUnregistered{


    address iexecOracleAddress;
    uint256 dappPrice;
    string dappName;

    //constructor
    function IexecOracleAPIUnregistered(address _iexecOracleAddress, uint256 _dappPrice, string _dappName) public {
        iexecOracleAddress=_iexecOracleAddress;
        dappPrice=_dappPrice;
        dappName=_dappName;
        //test with no registration for test purpuse
    }

    function iexecSubmit(string param) public payable{
        IexecOracle iexecOracle = IexecOracle(iexecOracleAddress);
        require(iexecOracle.submit(param));
    }


}
